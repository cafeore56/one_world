; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="1つの世界"
config/features=PackedStringArray("4.5", "Mobile")
config/icon="res://icon.svg"

[autoload]

EventBus="*res://autoload/event_bus.gd"
SaveLoader="*res://autoload/save_loader.gd"
SceneManager="*res://autoload/scene_manager.tscn"
TimerManager="*res://autoload/timer_manager.gd"

[debug]

gdscript/warnings/integer_division=0

[display]

window/size/viewport_width=512
window/size/viewport_height=1024
window/stretch/mode="canvas_items"
window/handheld/orientation=1

[editor_plugins]

enabled=PackedStringArray("res://addons/AdmobPlugin/plugin.cfg")

[input_devices]

pointing/emulate_touch_from_mouse=true

[rendering]

textures/canvas_textures/default_texture_filter=0
renderer/rendering_method="mobile"
textures/vram_compression/import_etc2_astc=true
